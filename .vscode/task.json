{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "watch-run",
      "command": "dotnet",
      "type": "process",
      "args": ["watch", "run"],
      "problemMatcher": "$msCompile",
      "isBackground": true
    },
    {
      "label": "build",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/bin\\Debug\\net6.0\\WsProjet.dll.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile"
    }
  ]
}
